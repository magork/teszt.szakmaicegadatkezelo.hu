* {
	margin: 0px;
	padding: 0px;
}

body, html{
	color: #2d1d0e;
	font-family: verdana;
	font-size: 12px;
  background-image: url( images/bg.jpg);
	background-color: #000;
}
a{
	color: #2d1d0e;
	text-decoration: none;
}
img{
  border: none;
}
h3.sep{
  width: 100%;
  background-image: url( images/diszito.png );
  background-position: top center;
	background-repeat: no-repeat;
	height: 28px;
}
* html h3.sep{
	background-color: transparent;
	background-image: none;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/diszito.png", sizingMethod="crop");
}

input, textarea{
	border: 1px solid #777;
	letter-spacing: 1px;
	width: 130px;
	padding: 2px;
	background: transparent;
	color: #ffd135;
	font-size: 12px;
}
#menu ul li{
  list-style-type: none;
	float: left;
  height: 49px;
}

#menu ul li a{
  text-decoration: none;
  color: #666;
  height: 49px;
  font-weight: bold;
}
td.thead{
  padding-left: 30px;
  padding-top: 10px;
}
td.hhead{
  padding-left: 5px;
  padding-top: 10px;
}
td.leiras{
  padding-top: 10px;
}
td{
  line-height: 18px;
}
td input, textarea{
  width: 65%;
  color: #2d1d0e;
  border: 1px solid #2d1d0e;
}
